{
  "Patterns": [
    {
      "Pattern": "Rename class-instance-creation",
      "Mappings": [
        {
          "Name": "class-instance-creation",
          "Before": "new :[4](:[3])",
          "After": "new :[2](:[3])"
        },
        {
          "Name": "Member-Reference",
          "Before": ":[5](:[2]::new):[3]",
          "After": ":[5](:[4]::new):[3]"
        }
      ]
    },

    {
    "Pattern": "Rename call-site",
    "Mappings": [
      {
        "Name": "Method-Invocation",
        "Before": ":[[1]].:[2](:[3])",
        "After": ":[[1]].:[4](:[3])"
      },
      {
        "Name": "Update-wrapper",
        "Before": ":[6~\\w+].:[2](:[1])",
        "After": ":[5~\\w+].:[4](:[1])"
      },
      {
        "Name": "Member-Reference",
        "Before": ":[1](:[3]:::[4]):[5]",
        "After": ":[m1](:[m3]:::[7]):[5]"
      }
    ]
  },
  {
    "Pattern": "Instance call-site to Static call-site",
    "Mappings": [
      {
        "Name": "Instance-Call-Site",
        "Before": ":[1].:[2](:[3])",
        "After": ":[4~\\w+].:[5](:[1])"
      },
      {
        "Name": "Member-Reference",
        "Before": ":[1](:[2]:::[3]):[4]",
        "After": ":[1](:[5]:::[6]):[4]"
      }
    ]
  },
  {
    "Pattern": "Assignment to setter",
    "Mappings": [
      {
        "Name": "Assignment-2-Set",
        "Before": ":[1]=:[2]",
        "After": ":[1].:[3](:[2])"
      }
    ]
  },
  {
    "Pattern": "New Instance Creation to instance method",
    "Mappings": [
      {
        "Name": "New-Instance-Creation-2-Instance-Method",
        "Before": "new :[2](:[1],:[3])",
        "After": ":[1].:[4](:[3])"
      }
    ]
  },
  {
      "Pattern": "Wrap",
      "Mappings": [
        {
          "Name": "Wrap",
          "Before": ":[1]",
          "After": ":[3~\\w+].:[2](:[1])"
        }
      ]
  },
    {
      "Pattern": "Introduce Casting",
      "Mappings": [
        {
          "Name": "cast",
          "Before": ":[1]",
          "After": "(:[2]):[1]"
        }
      ]
    },
  {
      "Pattern": "Add-method-chain",
      "Mappings": [
        {
          "Name": "add method chain1",
          "Before": ":[1]",
          "After": ":[1].:[2](:[4])"
        },
        {
          "Name": "add method chain2",
          "Before": ":[1].:[2](:[5])",
          "After": ":[1].:[2](:[5]).:[3](:[4])"
        }
      ]
    },
  {
      "Pattern": "Remove-method-chain",
      "Mappings": [
        {
          "Name": "Remove method chain1",
          "Before": ":[1~\\w+].:[2]()",
          "After": ":[1]"
        },
        {
          "Name": "remove method chain2",
          "Before": ":[1~\\w+].:[3]().:[2]",
          "After": ":[1].:[2]"
        }
      ]
    },

  {
      "Pattern": "Remove Casting",
      "Mappings": [
        {
          "Name": "un-cast",
          "Before": "(:[2]):[1]",
          "After": ":[1]"
        }
      ]
  },
  {
    "Pattern": "UnWrap",
    "Mappings": [
      {
        "Name": "UnWrap",
        "Before": ":[3~\\w+].:[2_.](:[1])",
        "After": ":[1]"
      }
    ]
  } 
  ]
}