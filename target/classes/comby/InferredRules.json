{
  "java.io.File->java.nio.file.Path": [
    {
      "Name": "New Instance Creation to instance method:New Instance Creation to instance method",
      "Match": "new File(:[1],:[3])",
      "Replace": ":[1].resolve(:[3])"
    },
    {
      "Name": "Add-method-chain:Add-method-chain",
      "Match": ":[1].:[2]()",
      "Replace": ":[1].:[2]().toString()"
    },
    {
      "Name": "Instance call-site to Static call-site:Instance call-site to Static call-site",
      "Match": ":[1].length()",
      "Replace": ":[4~\\w+].size(:[1])"
    },
    {
      "Name": "Instance call-site to Static call-site:Instance call-site to Static call-site",
      "Match": ":[1].isDirectory()",
      "Replace": ":[4~\\w+].isDirectory(:[1])"
    },
    {
      "Name": "Instance call-site to Static call-site:Instance call-site to Static call-site",
      "Match": ":[1].exists()",
      "Replace": ":[4~\\w+].exists(:[1])"
    },
    {
      "Name": "Instance call-site to Static call-site:Instance call-site to Static call-site",
      "Match": ":[1].exists()",
      "Replace": ":[4~\\w+].readAllBytes(:[1])"
    },
    {
      "Name": "Remove-method-chain:Remove-method-chain",
      "Match": ":[1~\\w+].listFiles()",
      "Replace": ":[1]"
    },
    {
      "Name": "Add-method-chain:Add-method-chain",
      "Match": ":[1]",
      "Replace": ":[1].toFile()"
    },
    {
      "Name": "Rename call-site:Rename call-site",
      "Match": ":[1].getName()",
      "Replace": ":[1].getFileName()"
    },
    {
      "Name": "Remove-method-chain:Remove-method-chain",
      "Match": ":[1~\\w+].getAbsolutePath()",
      "Replace": ":[1]"
    }
  ],
  "java.nio.file.Path->java.io.File": [
    {
      "Name": "Rename call-site:Rename call-site",
      "Match": ":[1].toString()",
      "Replace": ":[1].getPath()"
    },
    {
      "Name": "Instance call-site to Static call-site:Instance call-site to Static call-site",
      "Match": ":[1].toFile()",
      "Replace": ":[4~\\w+].deleteDirectory(:[1])"
    },
    {
      "Name": "UnWrap:UnWrap",
      "Match": "serialization.loadRecordsFromFile(:[1])",
      "Replace": ":[1]"
    },
    {
      "Name": "New Instance Creation to instance method:New Instance Creation to instance method",
      "Match": "new FileRoleRepository(:[1],logProvider)",
      "Replace": ":[1].getAbsolutePath()"
    },
    {
      "Name": "Instance call-site to Static call-site:Instance call-site to Static call-site",
      "Match": ":[1].toFile()",
      "Replace": ":[4~\\w+].newEmbeddedDatabaseBuilder(:[1])"
    },
    {
      "Name": "Instance call-site to Static call-site:Instance call-site to Static call-site",
      "Match": ":[1].toFile()",
      "Replace": ":[4~\\w+].fileSystem().deleteRecursively(:[1])"
    }
  ]
}