{
  "Patterns": [
  {
    "Pattern": "Remove-method-chain",
    "Mappings": [
      {
        "Name": "Remove method chain1",
        "Before": ":[1].:[2]()",
        "After": ":[1]"
      },
      {
        "Name": "remove method chain2",
        "Before": ":[1].:[3]().:[2]",
        "After": ":[1].:[2]"
      }
    ]
  },
  {
      "Pattern": "Rename call-site",
      "Mappings": [
        {
          "Name": "Method-Invocation",
          "Before": ":[1].:[2](:[3])",
          "After": ":[1].:[4](:[3])"
        },
        {
          "Name": "Update-wrapper",
          "Before": ":[6].:[2](:[1])",
          "After": ":[m6].:[m2](:[1])"
        },
        {
          "Name": "Member-Reference",
          "Before": ":[3](:[1]:::[4]):[5]",
          "After": ":[3](:[1]:::[7]):[5]"
        }
      ]
  },
  {
    "Pattern": "Rename class-instance-creation",
    "Mappings": [
      {
        "Name": "class-instance-creation",
        "Before": "new :[1](:[3])",
        "After": "new :[2](:[3])"
      },
      {
        "Name": "Member-Reference",
        "Before": ":[1](:[2]::new):[3]",
        "After": ":[1](:[4]::new):[3]"
      }
    ]
  },
  {
    "Pattern": "Class instance creation to Static call-site",
    "Mappings": [
      {
        "Name": "Class-Instance-Creation",
        "Before": "new :[1](:[3])",
        "After": ":[4~[A-Z]*\\w*].:[2](:[3])"
      },
      {
        "Name": "Member-Reference",
        "Before": ":[1](:[2]::new):[3]",
        "After": ":[1](:[4]:::[5]):[3]"
      }
    ]
  },
  {
    "Pattern": "New Instance Creation to instance method",
    "Mappings": [
      {
        "Name": "New-Instance-Creation-2-Instance-Method",
        "Before": "new :[1](:[2],:[3])",
        "After": ":[2].:[4](:[3])"
      }
    ]
  },
  {
    "Pattern": "Convert to Long literal",
    "Mappings": [
      {
        "Name": "Natural number",
        "Before": ":[1~\\d+]",
        "After": ":[1~\\d+]L"
      },
      {
        "Name": "Hexadecimal",
        "Before": ":[1~0[xX][0-9a-fA-F]+]",
        "After": ":[1~0[xX][0-9a-fA-F]+]L"
      },
      {
        "Name": "Hexadecimal",
        "Before": ":[1~0[xX][0-9a-fA-F]+]",
        "After": ":[1~0[xX][0-9a-fA-F]+]l"
      }
    ]
  },
  {
      "Pattern": "Wrap",
      "Mappings": [
        {
          "Name": "Wrap",
          "Before": ":[1]",
          "After": ":[2].(:[1])"
        }
      ]
  },
  {
      "Pattern": "UnWrap",
      "Mappings": [
        {
          "Name": "UnWrap",
          "Before": ":[2].(:[1])",
          "After": ":[1]"
        }
      ]
  },
  {
      "Pattern": "Add-method-chain",
      "Mappings": [
        {
          "Name": "add method chain",
          "Before": ":[1]",
          "After": ":[1].:[2](:[4])"
        },
        {
          "Name": "add method chain",
          "Before": ":[1].[2]",
          "After": ":[1].:[3](:[4]).:[2]"
        }
      ]
  }
  ]
}