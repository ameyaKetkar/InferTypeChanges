{
  "Patterns": [
  {
    "Pattern": "Rename call-site",
    "Mappings": [
      {
        "Name": "Method-Invocation",
        "Before": ":[1].:[2](:[3])",
        "After": ":[1].:[4](:[3])"
      },
      {
        "Name": "Update-wrapper",
        "Before": ":[1~[A-Z]*\\w*].:[2](:[3])",
        "After": ":[5~[A-Z]*\\w*].:[4](:[3])"
      },
      {
        "Name": "Member-Reference",
        "Before": ":[1](:[3]:::[4]):[5]",
        "After": ":[m1](:[m3]:::[7]):[5]"
      }
    ]
  },
  {
    "Pattern": "Rename class-instance-creation",
    "Mappings": [
      {
        "Name": "class-instance-creation",
        "Before": "new :[1](:[3])",
        "After": "new :[2](:[3])"
      },
      {
        "Name": "Member-Reference",
        "Before": ":[1](:[2]::new):[3]",
        "After": ":[1](:[4]::new):[3]"
      }
    ]
  },
  {
    "Pattern": "Class instance creation to Static call-site",
    "Mappings": [
      {
        "Name": "Class-Instance-Creation",
        "Before": "new :[1]([:3])",
        "After": ":[4~[A-Z]*\\w*].:[2]([:3])"
      },
      {
        "Name": "Member-Reference",
        "Before": ":[1](:[2]::new):[3]",
        "After": ":[1](:[4]:::[5]):[3]"
      }
    ]
  },
  {
    "Pattern": "Instance call-site to Static call-site",
    "Mappings": [
      {
        "Name": "Instance-Call-Site",
        "Before": ":[1].:[2](:[3])",
        "After": ":[4].:[5](:[1])"
      },
      {
        "Name": "Member-Reference",
        "Before": ":[1](:[2]:::[3]):[4]",
        "After": ":[1](:[5]:::[6]):[4]"
      }
    ]
  },
  {
    "Pattern": "New Instance Creation to instance method",
    "Mappings": [
      {
        "Name": "New-Instance-Creation-2-Instance-Method",
        "Before": "new :[1](:[2],:[3])",
        "After": ":[2].:[4](:[3])"
      }
    ]
  },

  {
    "Pattern": "Assignment to setter",
    "Mappings": [
      {
        "Name": "Assignment-2-Set",
        "Before": ":[1]=:[2]",
        "After": ":[1].:[3](:[2])"
      }
    ]
  },
  {
    "Pattern": "Convert to Long literal",
    "Mappings": [
      {
        "Name": "Long Literal",
        "Before": ":[1~\\d+]",
        "After": ":[1]L"
      }
    ]
  },{
      "Pattern": "Wrap",
      "Mappings": [
        {
          "Name": "Wrap",
          "Before": ":[1]",
          "After": ":[2](:[1])"
        }
      ]
    },
    {
      "Pattern": "UnWrap",
      "Mappings": [
        {
          "Name": "UnWrap",
          "Before": ":[2](:[1])",
          "After": ":[1]"
        }
      ]
    },
    {
      "Pattern": "Add-method-chain",
      "Mappings": [
        {
          "Name": "add method chain",
          "Before": ":[1]",
          "After": ":[1].:[2](:[4])"
        },
        {
          "Name": "add method chain",
          "Before": ":[1].[2]",
          "After": ":[1].:[3](:[4]).:[2]"
        }
      ]
    },
    {
      "Pattern": "Remove-method-chain",
      "Mappings": [
        {
          "Name": "Remove method chain",
          "Before": ":[1].:[2]()",
          "After": ":[1]"
        },
        {
          "Name": "add method chain",
          "Before": ":[1].:[3]().:[2]",
          "After": ":[1].[2]"
        }
      ]
    },
    {
      "Pattern": "Introduce Casting",
      "Mappings": [
        {
          "Name": "cast",
          "Before": ":[1]",
          "After": "(:[2]):[1]"
        }
      ]
    },
    {
      "Pattern": "Remove Casting",
      "Mappings": [
        {
          "Name": "un-cast",
          "Before": "(:[2]):[1]",
          "After": ":[1]"
        }
      ]
    }
  ]
}